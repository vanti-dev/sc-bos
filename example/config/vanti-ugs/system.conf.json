{
  "name": "van/uk/brum/ugs/eg-ac-01",
  "listenHTTPS": ":8443",
  "cors": {
    "debugMode": true,
    "corsOrigins": ["*"]
  },
  "staticHosting": [
    {"path": "/__/vanti-ugs/", "filePath": "./example/config/vanti-ugs"}
  ],
  "stores": {
    "postgres": {
      "uri": "postgres://postgres@localhost:5432/smart_core",
      "passwordFile": "./.data/secrets/postgres-password"
    }
  },
  "systems": {
    "alerts": {"storage": {"type": "postgres"}},
    "authn": {
      "user": {
        "localAccounts": true,
        "importFileAccounts": true,
        "fileAccounts": true
      },
      "system": {
        "tenantAccounts": true,
        "localAccounts": true
      }
    },
    "tenants": {"storage": {"type": "postgres"}}
  },
  "experimental": {
    "accounts": true,
    "simulateLatency": "5ms"
  }
}
