{
  "templates": {
    "fcu": {
      "selector": "#{{id}}",
      "sources": {
        "airTemperature": {
          "trait": "smartcore.traits.AirTemperature",
          "request": {"name": "van/uk/brum/ugs/devices/{{id}}"}
        }
      },
      "fill": {
        "source": {"ref": "airTemperature", "property": "ambientTemperature.valueCelsius"},
